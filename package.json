{
    "name": "advanced-filter-system",
    "version": "1.5.0",
    "description": "Advanced filtering system for DOM elements with multiple features",
    "main": "dist/afs.legacy.js",
    "module": "dist/afs.modern.js",
    "browser": "dist/afs.legacy.min.js",
    "private": false,
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/afs.modern.js",
            "require": "./dist/afs.legacy.js",
            "browser": "./dist/afs.legacy.min.js"
        }
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "homepage": "https://misits.github.io/advanced-filter-system",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/misits/advanced-filter-system.git"
    },
    "bugs": {
        "url": "https://github.com/misits/advanced-filter-system/issues"
    },
    "scripts": {
        "build": "rollup -c",
        "build:prod": "cross-env NODE_ENV=production rollup -c",
        "analyze": "open bundle-analysis.html",
        "test": "jest",
        "prepare": "npm run build:prod"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "filter",
        "filtering",
        "javascript",
        "dom",
        "sorting",
        "searching",
        "frontend",
        "browser",
        "manipulation",
        "element-filter",
        "web"
    ],
    "author": "misits",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.28.4",
        "@babel/plugin-transform-runtime": "^7.28.3",
        "@babel/preset-env": "^7.28.3",
        "@babel/runtime": "^7.28.4",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-strip": "^3.0.4",
        "@rollup/plugin-terser": "^0.4.4",
        "babel-jest": "^30.1.2",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "core-js": "^3.45.1",
        "cross-env": "^10.0.0",
        "jest": "^30.1.3",
        "jest-environment-jsdom": "^30.1.2",
        "rollup": "^4.50.1",
        "rollup-plugin-analyzer": "^4.0.0",
        "rollup-plugin-filesize": "^10.0.0",
        "rollup-plugin-visualizer": "^6.0.3"
    },
    "directories": {
        "example": "examples",
        "test": "tests"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "browserslist": [
        "> 0.25%",
        "not dead",
        "not ie 11"
    ]
}
